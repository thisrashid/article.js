{
  "tags": {
    "allowUnknownTags": true
  },
  "recurseDepth": 10,
  "source": {
    "include": [
      "./src/"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": false
    }
  },
  "opts": {
    "readme": "README.md",
    "recurse": true,
    "package": "package.json",
    "destination": "doc"
  }
}