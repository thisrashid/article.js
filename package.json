{
  "name": "article.js",
  "version": "0.0.1",
  "description": "Blogging platform on NodeJS, MySQL and Angular",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thisrashid/article.js.git"
  },
  "keywords": [
    "blog",
    "nodeblog",
    "blogjs",
    "jsblog"
  ],
  "author": "Rashid Azar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thisrashid/article.js/issues"
  },
  "homepage": "https://github.com/thisrashid/article.js#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "morgan": "^1.8.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  }
}
